{
	"name": "@enuesaa/kakkofn",
	"private": true,
	"type": "module",
	"version": "0.0.4",
	"scripts": {
		"dev": "rm -rf .svelte-kit && svelte-kit sync && vite dev --port 3000",
		"build": "vite build",
		"postbuild": "svelte-sitemap --domain https://kakkofn.dev --out-dir dist",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.76.3",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.9",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/node": "^20.12.12",
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.17",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.4.3",
		"tsx": "^4.10.5",
		"typescript": "^5.4.5",
		"vite": "^5.2.11"
	},
	"dependencies": {
		"buffer": "^6.0.3",
		"svelte-feather-icons": "^4.1.0"
	},
	"prettier": {
		"printWidth": 120,
		"tabWidth": 2,
		"semi": false,
		"singleQuote": true,
		"endOfLine": "lf",
		"trailingComma": "es5",
		"plugins": [
			"prettier-plugin-svelte"
		],
		"useTabs": true,
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	}
}
